   1607  unit    ghostit
   1608  calc    stats(0)--1
                         <  
   1609          i2--1
                   <  
   1610  doto    1,i1-0,23
                     <    
   1611  branch  xoc(i1)=.,1,x
                         p    
   1612  *branch  stat(i1),1,x
   1613  branch  hlp(i1),1,x
   1614  in      stat(i1)
   1615  branch  zreturn$and$stat(i1)/station,1,x
                                     =           
   1616          stats(i2-i2+1)-i1
                         <     <  
   1617  1
   1618  do      stats(0),x,getem

   1576  unit    explode
   1577  calc    wx1-20\dist\(-basz(i3=24)-spsz(i3=0))/wx1
                    <  /    /                             
   1578          tx-x1-ox
                   <     
   1579          ty-y1-oy
                   <     
   1580          tz-z1-oz
                   <     
   1581          k-dist/(tx\cx+ty\cy+tz\cz)
                  <        /     /     /   
   1582          tx-(((ox+k\ty)-vpx)\cy-((oy+k\ty)-vpy)\cx)\rs3
                   <       /        /         /        /   /   
   1583          ty-((oz+k\tz)-vpz)\rs3\dirflg
                   <      /        /   /      
   1584  doto    1,i5-0,20
                     <    
   1585  randu   wr
   1586  randu   tz
   1587  calc    wr-wr\wx1-wx1/2
                   <  /         
   1588          tz-tz\wx1-wx1/2
                   <  /         
   1589  gdraw   tx+wr,ty+tz;-wr+tx,-tz+ty
   1590  1
   1591  branch  op,x,1qu
   1592  1qu
